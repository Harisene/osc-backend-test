{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "paths": {
      "@cache/*": ["./cache/*"],
      "@directives/*": ["./directives/*"],
      "@graphql/*": ["./graphql/*"],
      "@middlewares/*": ["./middlewares/*"],
      "@models/*": ["./models/*"],
      "@utils/*": ["./utils/*"]
    }
  },
  "lib": ["es2015"]
}
